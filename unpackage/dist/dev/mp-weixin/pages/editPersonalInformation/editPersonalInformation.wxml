<view class="personalInformation"><view class="head"><navigator class="iconfont iconfanhui" open-type="navigateBack" url="../personal/personal"></navigator><text>编辑个人信息</text></view><view class="photo"><image class="img" src="{{imgSrc}}" data-event-opts="{{[['tap',[['changePhoto',['$event']]]]]}}" bindtap="__e"></image><view class="description">点击更换头像</view></view><view class="list"><view class="item"><view class="label">昵称</view><navigator class="value" open-type="navigate" url="../changeName/changeName?category=name">{{''+userInfo.name+''}}<view class="iconfont iconjinru"></view></navigator></view><view class="item"><view class="label">抖音号</view><navigator class="value" open-type="navigate" url="../changeName/changeName?category=account">{{''+userInfo.account+''}}<view class="iconfont iconjinru"></view></navigator></view><view class="item"><view class="label">简介</view><navigator class="value" open-type="navigate" url="../changeName/changeName?category=introduction">{{''+userInfo.introduction+''}}<view class="iconfont iconjinru"></view></navigator></view><picker range="{{schoolList}}" value="{{schoolListIndex}}" data-event-opts="{{[['change',[['chooseSchool',['$event']]]]]}}" bindchange="__e"><view class="item"><view class="label">学校</view><view class="value">{{''+(schoolList[schoolListIndex]||'无')+''}}<view class="iconfont iconjinru"></view></view></view></picker><picker range="{{sexList}}" value="{{sexListIndex}}" data-event-opts="{{[['change',[['chooseSex',['$event']]]]]}}" bindchange="__e"><view class="item"><view class="label">性别</view><view class="value">{{''+sexList[sexListIndex]+''}}<view class="iconfont iconjinru"></view></view></view></picker><picker mode="date" start="{{$root.m0}}" end="{{$root.m1}}" data-event-opts="{{[['change',[['dateChange',['$event']]]]]}}" bindchange="__e"><view class="item"><view class="label">生日</view><view class="value">{{''+(userInfo.birthDay||'')+''}}<view class="iconfont iconjinru"></view></view></view></picker><picker mode="region" data-event-opts="{{[['change',[['regionChange',['$event']]]]]}}" bindchange="__e"><view class="item"><view class="label">地区</view><view class="value">{{''+(userInfo.region||'无')+''}}<view class="iconfont iconjinru"></view></view></view></picker></view></view>